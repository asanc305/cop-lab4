@@ -0,0 +1,52 @@
+#include<linux/linkage.h>
+#include<linux/sched.h>
+
+int mystrcmp( char* str1, char* str2 )
+{
+  unsigned char *p1 = (unsigned char *) str1 ;
+  unsigned char *p2 = (unsigned char *) str2 ;
+
+  while(*p1 != '\0')
+  {
+    if( *p2 == '\0' ) return -1 ;
+    if( *p2 > *p1 ) return -1 ;
+    if( *p1 > *p2 ) return 1 ;
+
+    p1++ ;
+    p2++ ;
+  }
+
+  if( *p2 != '\0' ) return -1 ;
+
+  return 0 ;
+}
+
+asmlinkage long sys_sanchez(int i)
+{
+  struct task_struct *task ;
+  struct task_struct *t ;
+  char *s ;
+  unsigned long j ;
+  
+  for_each_process(task)
+  {
+    if( mystrcmp( task->comm, "part1" ) == 0 )
+    {
+      t = task ;
+      do 
+      {
+	j = t->state ;
+	if ( j == TASK_RUNNING ) s = "running" ;
+  	else if ( j == TASK_INTERRUPTIBLE ) s = "sleeping" ;
+  	else if ( j == TASK_UNINTERRUPTIBLE ) s = "suspended" ;
+  	else if ( j== TASK_STOPPED ) s = "stopped" ;
+
+        printk( "Name: %s \nPID: %i \nState: %s \nParentID: %i \n\n", t->comm, t->pid, s, t->parent->pid ) ;
+
+      }while_each_thread(task,t) ;        
+    } 
+  } 
+
+  return 0 ;
+
+}
